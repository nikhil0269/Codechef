#include <stdio.h>

#include<stdio.h>

int isTrueChef(char* arr)
{
    int c=0,h=0,e=0,f=0;
    int i;
    for(i=0;i<4;i++)
    {
        switch(arr[i])
        {
        case 'c':
            c=1;
            break;
        case 'h':
            h=1;
            break;
        case 'e':
            e=1;
            break;
        case 'f':
            f=1;
            break;
        default:break;

        }
    }
    if(c&&h&&e&&f)return 1;
    else
    {
       // c=0;h=0;e=0;f=0;
        return 0;
    }
}

int charIn(char ch,int a,int z)
{
    if(ch>=a&&ch<=z)return 1;
    else return 0;
}

void insertEnd(char*arr,char ch)
{
    int i;
    for(i=0;i<3;i++)
    {
        arr[i]=arr[i+1];
    }
    arr[i]=ch;
}

int main()
{
    int t,i,j,c;
    char arr[5];
    char str[200001];
    scanf("%d",&t);
    while(t--)
    {
        c=0;
        scanf("%s",str);
        i=0;j=0;
        for(j=0;j<4;j++)
        {
            if(charIn(str[i],65,90))
            {
                arr[i]=str[i]+32;
            }
            else if(charIn(str[i],97,122))
            {
                arr[j]=str[i];
            }
            i++;
        }
        if(isTrueChef(arr))
        {
            c++;
        }

        while(str[i]!='\0')
        {
            if(charIn(str[i],65,90))
            {
                insertEnd(arr,str[i]+32);
            }
            else if(charIn(str[i],97,122))
            {
                insertEnd(arr,str[i]);
            }
            if(isTrueChef(arr))c++;
            i++;
        }
        if(c==0) printf("normal\n");
        else printf("lovely %d\n",c);


    }
   return 0;
}




